<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.meetingsystemandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/png_phone"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/png_phone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        android:label="@string/app_name">
        <activity android:name=".activity_manager.ManagerActivity">
        </activity>
        <activity android:name=".user_history.UserHistoryActivity"></activity>
        <activity android:name=".activity_info.ShowActivityInfoActivity" android:label="@string/activity_info"></activity>
        <activity android:name=".personal_center.PercenalCenterctivity" />
        <activity android:name=".register.RegisterActivity" android:label="@string/register"/>
        <activity android:name=".login.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity" android:label="@string/main"/>
    </application>

</manifest>